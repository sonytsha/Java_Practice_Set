import java.util.Arrays;
public class Code_new_24 {
    public static void main ( String[] args) {
        // create an array
        int[] arr = { 1,2,3,546,4646};
        change(arr);
        System.out.println(Arrays.toString(arr));
    }

    static void change (int[] nums) {
        nums[0]=234; // if you make a change to the object via reference variable, it will change
    }
}
