import java.util.Scanner;
public class Code_new_6 {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        System.out.println("Output of for loop taking user input");
        // for loop
        for (int i = 1; i < n; i++) {
            System.out.println(i);
        }

        System.out.println("Output of for loop system input");

        for (int i = 1; i < 9; i++) {
            System.out.println( i);
        }

        System.out.println("Output of while loop");
        int num = 1;
        while (num <= 5) {
            System.out.println(num);
            num += 1;
        }
        System.out.println("Output of do - while loop");


        int number = 1;
        do {
            System.out.println(n);
            n++;
        } while (number <= 5);

        //// THE DIFFERENT BETWEEN DO WHULE AND WHILE LOOP IS , DO WHILE WILL EXECUTE ATLEAST ONCE
        int nu = 1;
        do {
            System.out.println("Hello Sony");
        } while (nu != 1);
    }
